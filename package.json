    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-sharing": "^14.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "xlsx": "^0.18.5"
