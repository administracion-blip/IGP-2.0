  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "api": "cd api && npm run dev",
    "dev": "concurrently -n api,expo -c blue,magenta \"npm run api\" \"expo start --web --port 8083\"",
